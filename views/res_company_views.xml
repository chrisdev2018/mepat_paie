<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="view_res_company_form" model="ir.ui.view">
            <field name="name">res.company.form</field>
            <field name="model">res.company</field>
            <field name="type">form</field>
            <field name="priority" eval="20"/>
            <field name="inherit_id" ref="base.view_company_form"/>
            <field name="arch" type="xml">

                <page string="Report Configuration" position="after">
                    <page string="Parametres DIPE">
                        <group>
                            <group>
                                <field name="numero_employeur" />
                                <field name="cle_numero_employeur" />
                                <field name="code_enregistrement" />
                                <field name="numero_dipe" />
                                <field name="cle_numero_dipe" />
                                
                            </group>
                            <group>
                                <field name="regime_employeur" />
                                <field name="numero_contribuable" />
                            </group>
                        </group>
                    </page>
                </page>

            </field>
        </record>

        
    </data>
</odoo>